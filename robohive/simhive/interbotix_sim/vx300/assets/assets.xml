<mujocoinclude>

    <compiler angle="radian"/>
    <!-- <option timestep="0.002" noslip_iterations="20"/> -->
    <option timestep="0.002"/>
    <size nuser_actuator="5"/>

    <asset>
        <mesh name="vx300_1_base" file="vx300/meshes/vx300_1_base.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300_2_shoulder" file="vx300/meshes/vx300_2_shoulder.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300_3_upper_arm" file="vx300/meshes/vx300_3_upper_arm.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300_4_forearm" file="vx300/meshes/vx300_4_forearm.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300_5_wrist" file="vx300/meshes/vx300_5_wrist.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300_6_gripper" file="vx300/meshes/vx300_6_gripper.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300_7_gripper_prop" file="vx300/meshes/vx300_7_gripper_prop.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300_8_gripper_bar" file="vx300/meshes/vx300_8_gripper_bar.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300_9_gripper_finger" file="vx300/meshes/vx300_9_gripper_finger.stl" scale="0.001 0.001 0.001" />
    </asset>

    <default>
        <default class="interbotix">
            <joint damping="5"/>
            <geom rgba="0.235 0.235 0.235 1"/>
        </default>
    </default>

    <sensor>
        <jointpos name="vx300_arm0_waist_pos" joint="vx300_arm0_waist"/>
        <jointpos name="vx300_arm0_shoulder_pos" joint="vx300_arm0_shoulder"/>
        <jointpos name="vx300_arm0_elbow_pos" joint="vx300_arm0_elbow"/>
        <jointpos name="vx300_arm0_wrist_angle_pos" joint="vx300_arm0_wrist_angle"/>
        <jointpos name="vx300_arm0_wrist_rotate_pos" joint="vx300_arm0_wrist_rotate"/>
        <jointpos name="vx300_arm0_gripper_pos" joint="vx300_arm0_gripper"/>
        <jointpos name="vx300_arm0_left_finger_pos" joint="vx300_arm0_left_finger"/>
        <jointpos name="vx300_arm0_right_finger_pos" joint="vx300_arm0_right_finger"/>

        <jointvel name="vx300_arm0_waist_vel" joint="vx300_arm0_waist"/>
        <jointvel name="vx300_arm0_shoulder_vel" joint="vx300_arm0_shoulder"/>
        <jointvel name="vx300_arm0_elbow_vel" joint="vx300_arm0_elbow"/>
        <jointvel name="vx300_arm0_wrist_angle_vel" joint="vx300_arm0_wrist_angle"/>
        <jointvel name="vx300_arm0_wrist_rotate_vel" joint="vx300_arm0_wrist_rotate"/>
        <jointvel name="vx300_arm0_gripper_vel" joint="vx300_arm0_gripper"/>
        <jointvel name="vx300_arm0_left_finger_vel" joint="vx300_arm0_left_finger"/>
        <jointvel name="vx300_arm0_right_finger_vel" joint="vx300_arm0_right_finger"/>
    </sensor>

    <actuator>
        <position name="vx300_arm0_waist_pos" joint="vx300_arm0_waist" kp="870" forcerange="-10 10" ctrlrange="-3.14158 3.14158" ctrllimited="true" forcelimited="true"/>
        <position name="vx300_arm0_shoulder_pos" joint="vx300_arm0_shoulder" kp="870" forcerange="-10 10" ctrlrange="-1.85005 1.25664" ctrllimited="true" forcelimited="true"/>
        <position name="vx300_arm0_elbow_pos" joint="vx300_arm0_elbow" kp="870" forcerange="-10 10" ctrlrange="-1.76278 1.6057" ctrllimited="true" forcelimited="true"/>
        <position name="vx300_arm0_wrist_angle_pos" joint="vx300_arm0_wrist_angle" kp="870" forcerange="-10 10" ctrlrange="-1.8675 2.23402" ctrllimited="true" forcelimited="true"/>
        <position name="vx300_arm0_wrist_rotate_pos" joint="vx300_arm0_wrist_rotate" kp="870" forcerange="-10 10" ctrlrange="-3.14158 3.14158" ctrllimited="true" forcelimited="true"/>
        <position name="vx300_arm0_gripper_pos" joint="vx300_arm0_gripper" kp="120" forcerange="-12 12" ctrllimited="false" forcelimited="true"/>
        <position name="vx300_arm0_left_finger_pos" joint="vx300_arm0_left_finger" kp="120" forcerange="-12 12" ctrlrange="0.021 0.057" ctrllimited="true" forcelimited="true"/>
        <position name="vx300_arm0_right_finger_pos" joint="vx300_arm0_right_finger" kp="120" forcerange="-12 12" ctrlrange="-0.057 -0.021" ctrllimited="true" forcelimited="true"/>
    </actuator>

</mujocoinclude>
