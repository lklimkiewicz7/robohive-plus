<mujocoinclude>

    <compiler angle="radian"/>
    <option timestep="0.002"/>
    <size nuser_actuator="6"/>

    <asset>
        <mesh name="vx300s_1_base" file="vx300s/meshes/vx300s_1_base.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300s_2_shoulder" file="vx300s/meshes/vx300s_2_shoulder.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300s_3_upper_arm" file="vx300s/meshes/vx300s_3_upper_arm.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300s_4_upper_forearm" file="vx300s/meshes/vx300s_4_upper_forearm.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300s_5_lower_forearm" file="vx300s/meshes/vx300s_5_lower_forearm.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300s_6_wrist" file="vx300s/meshes/vx300s_6_wrist.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300s_7_gripper" file="vx300s/meshes/vx300s_7_gripper.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300s_8_gripper_prop" file="vx300s/meshes/vx300s_8_gripper_prop.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300s_9_gripper_bar" file="vx300s/meshes/vx300s_9_gripper_bar.stl" scale="0.001 0.001 0.001" />
        <mesh name="vx300s_10_gripper_finger" file="vx300s/meshes/vx300s_10_gripper_finger.stl" scale="0.001 0.001 0.001" />
    </asset>

    <default>
        <default class="interbotix">
            <joint damping="5"/>
            <geom rgba="0.235 0.235 0.235 1"/>
            <default class="finger_collision">
                <geom friction="0.9 0.1 0.1" priority="1" />
            </default>
            <default class="finger_joint">
                <joint damping="20" />
            </default>
        </default>
    </default>

</mujocoinclude>
