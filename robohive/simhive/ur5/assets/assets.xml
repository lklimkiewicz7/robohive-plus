<mujocoinclude>

    <!-- ARM -->
    <asset>
        <material class="ur5e" name="black" rgba="0.033 0.033 0.033 1" />
        <material class="ur5e" name="jointgray" rgba="0.278 0.278 0.278 1" />
        <material class="ur5e" name="linkgray" rgba="0.82 0.82 0.82 1" />
        <material class="ur5e" name="urblue" rgba="0.49 0.678 0.8 1" />

        <mesh file="meshes/base_0.obj" />
        <mesh file="meshes/base_1.obj" />
        <mesh file="meshes/shoulder_0.obj" />
        <mesh file="meshes/shoulder_1.obj" />
        <mesh file="meshes/shoulder_2.obj" />
        <mesh file="meshes/upperarm_0.obj" />
        <mesh file="meshes/upperarm_1.obj" />
        <mesh file="meshes/upperarm_2.obj" />
        <mesh file="meshes/upperarm_3.obj" />
        <mesh file="meshes/forearm_0.obj" />
        <mesh file="meshes/forearm_1.obj" />
        <mesh file="meshes/forearm_2.obj" />
        <mesh file="meshes/forearm_3.obj" />
        <mesh file="meshes/wrist1_0.obj" />
        <mesh file="meshes/wrist1_1.obj" />
        <mesh file="meshes/wrist1_2.obj" />
        <mesh file="meshes/wrist2_0.obj" />
        <mesh file="meshes/wrist2_1.obj" />
        <mesh file="meshes/wrist2_2.obj" />
        <mesh file="meshes/wrist3.obj" />
    </asset>

    <!-- CAMERA -->
    <asset>
        <material class="d435i" name="Black_Acrylic" rgba="0.070360 0.070360 0.070360 1" />
        <material class="d435i" name="Cameras_Gray" rgba="0.296138 0.296138 0.296138 1" />
        <material class="d435i" name="IR_Emitter_Lens" rgba="0.287440 0.665387 0.327778 1" />
        <material class="d435i" name="IR_Lens" rgba="0.035601 0.035601 0.035601 1" />
        <material class="d435i" name="IR_Rim" rgba="0.799102 0.806952 0.799103 1" />
        <material class="d435i" name="Metal_Casing" rgba="0.7 0.7 0.7 1" />
        <material class="d435i" name="RGB_Pupil" rgba="0.087140 0.002866 0.009346 1" />
        <mesh name="d435i_0" file="realsense/d435i_0.obj" />
        <mesh name="d435i_1" file="realsense/d435i_1.obj" />
        <mesh name="d435i_2" file="realsense/d435i_2.obj" />
        <mesh name="d435i_3" file="realsense/d435i_3.obj" />
        <mesh name="d435i_4" file="realsense/d435i_4.obj" />
        <mesh name="d435i_5" file="realsense/d435i_5.obj" />
        <mesh name="d435i_6" file="realsense/d435i_6.obj" />
        <mesh name="d435i_7" file="realsense/d435i_7.obj" />
        <mesh name="d435i_8" file="realsense/d435i_8.obj" />
    </asset>

    <!-- GRIPPER -->
    <asset>
        <material name="gripper" rgba="0.6 0.6 0.6 1" reflectance="0.1" />
        <material name="milibar" rgba="0.05 0.05 0.05 1" reflectance="0.4" />
        <material name="milibar_connector" rgba="0.4 0.4 0.45 1" reflectance="0.0" />
        <mesh name="base" file="wsg50/WSG50_110.stl" />

        <texture type="2d" name="texture" file="finray/one_piece_wide_grasp_finray_finger_color.png" />
        <material name="finray_material" texture="texture" specular="0.5" shininess="0.0" />
        <mesh name="finger" file="finray/one_piece_wide_grasp_finray_finger.obj" scale="0.76923076923 1.0 1.0" />
        <mesh name="finger_collider" file="finray/collider.obj" scale="0.76923076923 1.0 1.0" />

        <mesh name="mount" file="wsg50/mount.obj" />
        <mesh name="mount_collider_1" file="wsg50/mount_collider_1.obj" />
        <mesh name="mount_collider_2" file="wsg50/mount_collider_2.obj" />
    </asset>

    <!-- ARM -->
    <default>
        <default class="ur5e">
            <material specular="0.5" shininess="0.25" />
            <joint axis="0 1 0" range="-6.28319 6.28319" armature="0.1" />
            <general gaintype="fixed" biastype="affine" ctrlrange="-6.2831 6.2831" gainprm="2000" biasprm="0 -2000 -400" forcerange="-150 150" />
            <default class="size3">
                <default class="size3_limited">
                    <joint range="-3.1415 3.1415" />
                    <general ctrlrange="-3.1415 3.1415" />
                </default>
            </default>
            <default class="size1">
                <general gainprm="500" biasprm="0 -500 -100" forcerange="-28 28" />
            </default>
            <default class="visual">
                <geom type="mesh" contype="0" conaffinity="0" group="2" />
            </default>
            <default class="collision">
                <geom type="capsule" group="3" />
                <default class="eef_collision">
                    <geom type="cylinder" />
                </default>
            </default>
            <site size="0.001" rgba="0.5 0.5 0.5 0.3" group="4" />
        </default>
    </default>

    <!-- CAMERA -->
    <default>
        <default class="d435i">
            <material specular="0" shininess="0.25" />
            <default class="camera_visual">
                <geom group="2" type="mesh" contype="0" conaffinity="0" mass="0" />
            </default>
            <default class="camera_collision">
                <geom group="3" type="mesh" mass="0" />
            </default>
        </default>
    </default>

    <!-- GRIPPER -->
    <default>
        <default class="gripper_visual">
            <geom type="mesh" contype="0" conaffinity="0" group="2" />
        </default>
        <default class="gripper_collision">
            <geom type="mesh" group="3" />
        </default>
        <default class="finray_collision">
            <geom condim="6" friction="0.9 0.1 0.1" group="3" priority="1" />
        </default>
    </default>

    <sensor>
        <jointpos name="shoulder_pan_pos" joint="shoulder_pan_joint"/>
        <jointpos name="shoulder_lift_pos" joint="shoulder_lift_joint"/>
        <jointpos name="elbow_pos" joint="elbow_joint"/>
        <jointpos name="wrist_1_pos" joint="wrist_1_joint"/>
        <jointpos name="wrist_2_pos" joint="wrist_2_joint"/>
        <jointpos name="wrist_3_pos" joint="wrist_3_joint"/>
        <jointpos name="gripper_pos" joint="right_driver_joint"/>
    </sensor>

    <actuator>
        <general class="size3" name="shoulder_pan" joint="shoulder_pan_joint" />
        <general class="size3" name="shoulder_lift" joint="shoulder_lift_joint" />
        <general class="size3_limited" name="elbow" joint="elbow_joint" />
        <general class="size1" name="wrist_1" joint="wrist_1_joint" />
        <general class="size1" name="wrist_2" joint="wrist_2_joint" />
        <general class="size1" name="wrist_3" joint="wrist_3_joint" />
        <position name="gripper" kp="2000" forcerange="-80 80" ctrlrange="0 0.055" joint="right_driver_joint" />
    </actuator>

    <equality>
        <joint joint1="right_driver_joint" joint2="left_driver_joint"
            solimp="0.8 0.85 0.001" solref="0.005 1" />
    </equality>
    
</mujocoinclude>
