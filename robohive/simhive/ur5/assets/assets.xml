<mujocoinclude>

    <!-- <compiler angle="radian" autolimits="true" />
    <visual>
        <quality offsamples="0"/>
    </visual>
    <option integrator="implicitfast" /> -->

    <asset>
        <material class="ur5e" name="black" rgba="0.033 0.033 0.033 1" />
        <material class="ur5e" name="jointgray" rgba="0.278 0.278 0.278 1" />
        <material class="ur5e" name="linkgray" rgba="0.82 0.82 0.82 1" />
        <material class="ur5e" name="urblue" rgba="0.49 0.678 0.8 1" />

        <mesh file="meshes/base_0.obj" />
        <mesh file="meshes/base_1.obj" />
        <mesh file="meshes/shoulder_0.obj" />
        <mesh file="meshes/shoulder_1.obj" />
        <mesh file="meshes/shoulder_2.obj" />
        <mesh file="meshes/upperarm_0.obj" />
        <mesh file="meshes/upperarm_1.obj" />
        <mesh file="meshes/upperarm_2.obj" />
        <mesh file="meshes/upperarm_3.obj" />
        <mesh file="meshes/forearm_0.obj" />
        <mesh file="meshes/forearm_1.obj" />
        <mesh file="meshes/forearm_2.obj" />
        <mesh file="meshes/forearm_3.obj" />
        <mesh file="meshes/wrist1_0.obj" />
        <mesh file="meshes/wrist1_1.obj" />
        <mesh file="meshes/wrist1_2.obj" />
        <mesh file="meshes/wrist2_0.obj" />
        <mesh file="meshes/wrist2_1.obj" />
        <mesh file="meshes/wrist2_2.obj" />
        <mesh file="meshes/wrist3.obj" />
    </asset>

    <default>
        <default class="ur5e">
            <material specular="0.5" shininess="0.25" />
            <joint axis="0 1 0" range="-6.28319 6.28319" armature="0.1" />
            <general gaintype="fixed" biastype="affine" ctrlrange="-6.2831 6.2831" gainprm="2000" biasprm="0 -2000 -400" forcerange="-150 150" />
            <default class="size3">
                <default class="size3_limited">
                    <joint range="-3.1415 3.1415" />
                    <general ctrlrange="-3.1415 3.1415" />
                </default>
            </default>
            <default class="size1">
                <general gainprm="500" biasprm="0 -500 -100" forcerange="-28 28" />
            </default>
            <default class="visual">
                <geom type="mesh" contype="0" conaffinity="0" group="2" />
            </default>
            <default class="collision">
                <geom type="capsule" group="3" />
                <default class="eef_collision">
                    <geom type="cylinder" />
                </default>
            </default>
            <site size="0.001" rgba="0.5 0.5 0.5 0.3" group="4" />
        </default>
    </default>

    <sensor>
        <jointpos name="shoulder_pan_pos" joint="shoulder_pan_joint"/>
        <jointpos name="shoulder_lift_pos" joint="shoulder_lift_joint"/>
        <jointpos name="elbow_pos" joint="elbow_joint"/>
        <jointpos name="wrist_1_pos" joint="wrist_1_joint"/>
        <jointpos name="wrist_2_pos" joint="wrist_2_joint"/>
        <jointpos name="wrist_3_pos" joint="wrist_3_joint"/>
    </sensor>

    <actuator>
        <general class="size3" name="shoulder_pan" joint="shoulder_pan_joint" />
        <general class="size3" name="shoulder_lift" joint="shoulder_lift_joint" />
        <general class="size3_limited" name="elbow" joint="elbow_joint" />
        <general class="size1" name="wrist_1" joint="wrist_1_joint" />
        <general class="size1" name="wrist_2" joint="wrist_2_joint" />
        <general class="size1" name="wrist_3" joint="wrist_3_joint" />
    </actuator>

</mujocoinclude>
