<mujoco model="UR5 Reach">
    <compiler angle="radian" meshdir="../../../simhive/ur5" texturedir="../../../simhive/ur5" autolimits="true"/>
    <visual>
        <quality offsamples="0" numquads="32"/>
        <headlight diffuse="0.6 0.6 0.6" ambient="0.6 0.6 0.6" specular="0.6 0.6 0.6" />
        <global azimuth="120" elevation="-20" offheight="512" />
    </visual>
    <option integrator="implicitfast" />
    <size njmax='1000' nconmax='1000'/>

    <!-- <include file="../../../simhive/scene_sim/topfloor_scene.xml"/> -->
    <include file="../../../simhive/ur5/assets/assets.xml"/>
    <include file="../../../simhive/custom/table/assets.xml"/>

    <default>
        <default class="realsense">
            <camera fovy="42.5" />
        </default>
    </default>
    <asset>
        <material name="floor" rgba="0.95 0.95 0.95 1" reflectance="0" shininess="0"
            specular="0" />
        <material name="wall"
            rgba="1 1 1 1"
            reflectance="0" shininess="0"
            specular="0" />
        <texture type="skybox" builtin="flat" rgb1="1 1 1" rgb2="1 1 1" width="8"
            height="16" />
    </asset>

    <worldbody>

        <geom name="floor" size="0 0 0.05" type="plane" material="floor" pos="0 0 -0.3" />
        <geom name="wall" size="0 0 0.05" type="plane" material="wall" pos="-0.6 0 0.4" euler="0 1.57 0" />
        <camera class="realsense" name="front" pos="1.4 0.0 0.8" xyaxes="0 1.000 0.000 -0.4 0.0 0.8" />
        <camera class="realsense" name="top_down" pos="0.4 0.0 0.92"
            xyaxes="0.0 -1.000 0.0 1.000 0.0 0.0" />

        <site name='workspace' type='box' size='.6 .375 .25' pos='0 0.475 1.0' group='3' rgba='0 0 .4 .2'/>

        <!-- Robot -->
        <body pos='0 0 0' euler='0 0 0'>
            <include file="../../../simhive/ur5/assets/arm.xml"/>
        </body>

        <!-- Table -->
        <body pos='0.4 0 0'>
            <include file="../../../simhive/custom/table/table.xml"/>
        </body>

        <!-- tasks details added here via include-->
        <site name='target' type='sphere' size='.1' pos='0 0.475 1.0' group='1' rgba='.1 .8 .2 .4'/>

    </worldbody>

</mujoco>